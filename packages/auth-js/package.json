    "scripts": {
        "lint": "eslint --ext .js,.ts .",
        "fix-lint": "eslint --ext .js,.ts . --fix",
        "build": "cross-env NODE_ENV=production rimraf dist && rimraf umd && npm run type-check && rollup -c",
