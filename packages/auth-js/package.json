        "Asgardeo",
        "authentication",
        "oidc",
        "oauth2",
        "spa",
        "node.js",
        "react-native"
    ],
    "main": "dist/asgardeo-oidc.production.js",
    "module": "dist/asgardeo-oidc.production.esm.js",
